        "darth-ce-start": "cross-env NODE_ENV=development CUSTOM_ENV=dev webpack-dev-server --progress --config ./webpack.dev.js ",
        "build:dev": "cross-env NODE_ENV=production CUSTOM_ENV=dev webpack --config ./webpack.prod.js ",
        "build:prod": "cross-env NODE_ENV=production CUSTOM_ENV=prod webpack --config ./webpack.prod.js ",
        "build:test": "cross-env NODE_ENV=production CUSTOM_ENV=test webpack --config ./webpack.prod.js ",
        "es": "rimraf es && npm run es:utils && npm run es:common && npm run es:home && npm run es:setting && npm run es:login && npm run es:assests && npm run es:css",
        "es:assests": "ncp src/assets es/assets",
        "es:common": "cross-env NODE_ENV=production babel src/common --out-dir es/common && babel src/routers.js --out-file es/routers.js && babel src/app.js --out-file es/app.js && babel src/export.js --out-file es/index.js",
        "es:utils": "cross-env NODE_ENV=production babel src/utils --out-dir es/utils",
        "es:home": "cross-env NODE_ENV=production babel src/Home --out-dir es/Home",
        "es:setting": "cross-env NODE_ENV=production babel src/setting --out-dir es/Setting",
        "es:login": "cross-env NODE_ENV=production babel src/login --out-dir es/Login",
        "es:prod": "npm run es:esm && npm run es:css",
        "es:esm": "rimraf es && cross-env NODE_ENV=production BABEL_ENV=esm rollup -c",
        "es:css": "cd scripts && gulp compile"
    },
