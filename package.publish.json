    "dependencies": {
        "doublekit-core-ui": "^1.0.0",
        "doublekit-message-ui": "^1.0.0",
        "doublekit-privilege-ui": "^1.0.0",
        "doublekit-plugin-ui": "^1.0.0",
        "doublekit-user-ui": "^1.0.0",
        "is-class": "^0.0.9"
    },
    "browserslist": [
        "defaults",
