    "description": "",
    "main": "distIndex.js",
    "scripts": {
        "build:dev": "cross-env NODE_ENV=production CUSTOM_ENV=dev webpack --config ./webpack.prod.js ",
        "build:test": "cross-env NODE_ENV=production CUSTOM_ENV=test webpack --config ./webpack.prod.js ",
        "build:prod": "cross-env NODE_ENV=production CUSTOM_ENV=prod webpack --config ./webpack.prod.js ",
        "build-test": "cross-env NODE_ENV=production webpack --config ./webpack.test.js ",
        "es": "rimraf dist/* && ./node_modules/.bin/babel src/ --copy-files  --extensions .js --out-dir dist/ && ncp src/dist.js dist/dist.js && ncp package.publish.json dist/package.json"
    },
    "peerDependencies": {
        "react-dom": ">=16.8.0",
        "react": ">=16.8.0"
    },
